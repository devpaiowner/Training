<IfModule mod_rewrite.c>
    Options -MultiViews
    RewriteEngine On
    RewriteCond %{HTTP_HOST} !=""
    RewriteCond %{HTTP_HOST} !^www. [NC]
    RewriteCond %{HTTPS}s ^on(s)|
    #RewriteRule ^ http://www./%%7BHTTP_HOST%7D%%7BREQUEST_URI%7D [R=301,L]

    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteRule ^ index.html [QSA,L]
</IfModule>

<Files ~ ".(env|config.js|md|gitignore|gitattributes|lock|log)$">
    Order allow,deny
    Deny from all
</Files>